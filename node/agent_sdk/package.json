{
  "name": "@moonshot-ai/kimi-agent-sdk",
  "version": "0.1.0",
  "description": "SDK for interacting with Kimi Code CLI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoonshotAI/kimi-agent-sdk.git",
    "directory": "node/agent_sdk"
  },
  "keywords": [
    "kimi",
    "moonshot",
    "ai",
    "agent",
    "sdk",
    "cli"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.mjs",
      "require": "./dist/schema.cjs"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "import": "./dist/errors.mjs",
      "require": "./dist/errors.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm run typecheck && pnpm run build",
    "publish:npm": "pnpm publish --access public --no-git-checks"
  },
  "dependencies": {
    "toml": "^3.0.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
